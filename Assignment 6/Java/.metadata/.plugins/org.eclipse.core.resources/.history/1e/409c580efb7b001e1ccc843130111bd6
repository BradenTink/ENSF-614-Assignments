import java.awt.Graphics;

class Decorator implements Component {
	protected int x;
    protected int y;
    protected int width;
    protected int height;
	protected Component cmp;
	

    public Decorator(Component component) {
		
    	cmp = component; 
    }
  

    @Override
	public void draw(Graphics g) {
		// TODO Auto-generated method stub
		  cmp.draw(g);
		
	}

  
}
